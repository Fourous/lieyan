/* pages/cardetailone/cardetailone.wxss */
.all{
  width: 100%;
  height: 100%;
}
.mapview {
  width: 100%;
  height: 300rpx;
}
.mapview.active {
  width: 100%;
  height: 95%;
  position: fixed;
  z-index: 111;
}

.Map{
  width: 100%;
  height: 100%;
  z-index: 1;
}

.downview {
  width: 100%;
  height: 60rpx;
  text-align: center;
  background: #eee;
}
.downbutton{
  width: 50rpx;
  height: 50rpx;
  line-height: 60rpx;
}
.upview {
  width: 100%;
  height: 5%;
  text-align: center;
  background: #eee;
  position: fixed;
  bottom: 0px;
  z-index: 111;
}
.upbutton{
  width: 50rpx;
  height: 50rpx;
  line-height: 60rpx;
}
.mapintro {
  height: 65rpx;
  width: 100%;
  text-align: left;
  font-size: 11px;
  border-bottom: 5rpx solid #ccc;
  padding-top: 20rpx;
}
.title{
  margin-top: 20rpx;
  text-align: center;
}
.titletxt{
  font-size: 20px;
  font-weight: bold;
}
.detailintro{
  margin-top: 20rpx;
  padding-left: 30rpx;
  padding-right: 30rpx;
}
.pic1 {
  height:450rpx;
  width: 100%;
}
.image1{
  height: 100%;
  width: 100%;
}
.txt1{
  width: 100%;
  /* text-align: center; */
}
.text1{
font-size: 12px;
font-weight: lighter;
}

